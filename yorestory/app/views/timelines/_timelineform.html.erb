<%= form_for timeline_local do |f| %>
  <%= f.text_field :title, {placeholder: "title"} %>
 <%= f.text_field :begin_date, {placeholder: "birth year"} %>
   <%= f.text_field :end_date, {placeholder: "death year or present"} %>
  <%= f.hidden_field :user_id, {value: user.id} %>

  <%= f.submit %>
<% end %>
